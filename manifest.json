{  "manifest_version": 3,  "name": "IqoTimer",  "version": "1.0.0",  "description": "IqoTimer timer help you to focus and get things done, Author: Anthony Fink",  "author": "Anthony Fink",  "icons": {    "16": "icon.png",    "48": "icon.png",    "128": "icon.png"  },  "action": {    "default_icon": "icon.png",    "default_title": "IqoTimer",    "default_popup": "popup/popup.html"  },  "permissions": ["storage","alarms","notifications"],  "background": {    "service_worker": "background.js"  },  "options_page": "options/options.html"}